{
  "extends": "@adonisjs/tsconfig/tsconfig.client.json",
  "compilerOptions": {
    "module": "ESNext",
    "jsx": "react-jsx",

    "paths": {
      "~/*": ["./*"],
      "#css/*": ["./css/*"],
      "#types/*": ["./utils/types/*"],
      "#assets/*": ["./assets/*"],
      "#schemas/*": ["../app/schemas/*"]
    }
  },
  "include": ["./**/*.ts", "./**/*.tsx"]
}
